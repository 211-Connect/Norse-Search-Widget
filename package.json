{
  "name": "search-widget",
  "version": "0.0.4",
  "description": "Embeddable search widget built with Preact and Vite",
  "type": "module",
  "main": "./dist/widget/search-widget.umd.js",
  "module": "./dist/widget/search-widget.es.js",
  "types": "./dist/widget/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/widget/search-widget.es.js",
      "require": "./dist/widget/search-widget.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "widget",
    "search",
    "embeddable",
    "preact",
    "vite"
  ],
  "author": "211-Connect",
  "license": "MIT",
  "dependencies": {
    "preact": "^10.28.3",
    "radash": "^12.1.1"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.3",
    "@types/node": "^24.10.0",
    "@vanilla-extract/css": "^1.18.0",
    "@vanilla-extract/vite-plugin": "^5.1.4",
    "serve": "^14.2.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
